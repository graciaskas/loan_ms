        <!------ Include the above in your HEAD tag ---------->
        <link rel="stylesheet" href="/css/profile.css">
        <link rel="stylesheet" href="/css/header.css">

        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Profile of <%=print[0].cus_name%></title>
    </head>
    <body>
        <nav class="navbar navbar-icon-top navbar-expand-lg navbar-dark bg-dark">
            <a class="navbar-brand text-info" href="#">MicroVest</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
        
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a class="nav-link" href="/index">
                            <i class="fa fa-home"></i>
                            Home
                            <span class="sr-only">(current)</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/view_scheme">
                            <i class="fa fa-envelope-o">
                                <!-- <span class="badge badge-danger">11</span> -->
                            </i>
                            Schemes
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/view_loan">
                            <i class="fa fa-globe">
                                <!-- <span class="badge badge-success">11</span> -->
                            </i>
                            Loans
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/installment2/<%=print[0].cus_id%>">
                            <i class="fa fa-money">
                                <!-- <span class="badge badge-success">11</span> -->
                            </i>
                            Installments
                        </a>
                    </li>
        
                </ul>
                <ul class="navbar-nav ">
                    <li class="nav-item">
                        <a class="nav-link" href="/generateReport/<%=print[0].cus_id%>" id="print">
                            <i class="fa fa-print text-success ">
                                <span class="badge badge-danger "><%=print[0].t%></span>
                            </i>
                            Print
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/logout">
                            <i class="fa fa-sign-out text-danger">
        
                            </i>
                            Logout
                        </a>
                    </li>
                </ul>
        
            </div>
        </nav>
        <div class="container">
        
            <div class="row profile">
        
                <div class="col-md-3">
        
                    <div class="profile-sidebar">
                        <!-- <div class="brand_logo_container">
                            <img src="/uploads/login-logo.png" class="brand_logo" alt="Logo">
                        </div> -->
                        <div class="profile-usertitle p-5">
                            <div class="profile-usertitle-name">
                                <img src="/uploads/<%=print[0].img%>" class="brand_logo" alt="Logo">
        
                            </div>
        
                        </div>
        
                        <div class="profile-usermenu">
                            <ul class="nav">
                                <li>
                                    <a href="#">
                                        <i class="glyphicon glyphicon-plus"></i>
                                        ID: <%=print[0].cus_id%> </a>
                                </li>
                                <li>
                                    <a href="/invoice/<%=print[0].cus_id%>">
                                        <i class="glyphicon glyphicon-user"></i>
                                        Name: <%=print[0].cus_name%> </a>
                                </li>
                                <li class="active">
                                    <a href="/invoice/<%=print[0].cus_id%>">
                                        <i class="glyphicon glyphicon-home"></i>
                                        Address: <%=print[0].cus_address%> </a>
                                </li>
                                <li class="active">
                                    <a href="/invoice/<%=print[0].cus_id%>">
                                        <i class="glyphicon glyphicon-phone"></i>
                                        Contact: <%=print[0].cus_contact%> </a>
                                </li>
        
                                <li>
                                    <a href="#" target="_blank">
                                        <i class="glyphicon glyphicon-gift"></i>
                                        Asset: <%=print[0].cus_asset%> </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="glyphicon glyphicon-euro"></i>
                                        Asset Price: <%=print[0].asset_price%> </a>
                                </li>
        
                            </ul>
        
                        </div>
                        <div class="profile-usertitle">
                            <div class="profile-usertitle-name">
                                Loan Info
                            </div>
                        </div>
                        <div class="profile-usermenu">
                            <ul class="nav">
                                <li>
                                    <a href="/view_scheme">
                                        <i class="glyphicon glyphicon-plus"></i>
                                        Scheme ID: <%=print[0].scheme_id%> </a>
                                </li>
                                <li class="active">
                                    <a href="/view_scheme">
                                        <i class="glyphicon glyphicon-user"></i>
                                        Name: <%=print[0].scheme_name%> </a>
                                </li>
                                <li class="active">
                                    <a href="#">
                                        <i class="glyphicon glyphicon-usd"></i>
                                        Amount: <%=print[0].scheme_amount%> </a>
                                </li>
        
                                <li>
                                    <a href="/installment2/<%=print[0].cus_id%>" target="_blank">
                                        <i class="glyphicon glyphicon-euro"></i>
                                        Installment: <%=print[0].installment_amount%> </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="glyphicon glyphicon-time"></i>
                                        Taken Date: <%=print[0].date%> </a>
                                </li>
        
                            </ul>
        
                        </div>
        
                    </div>
                </div>
        
                <div class="col-md-9">
                    <div class="profile-content">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th scope="col">Installment NO#</th>
                                    <th scope="col">Installment Dates</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% var cnt=1 %>
                                <% print.forEach(function (user) { %>
                                <tr>
                                    <td><%= cnt++ %></td>
        
        
                                    <td><%= user.Time %></td>
        
        
                                    <% }) %>
        
                                </tr>
        
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <center>
            <strong>Powered by <a href="#" target="_blank">Kh Nishad</a></strong>
        </center>
        <br>
        <br>

